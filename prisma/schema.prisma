// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Vote {
  id        String   @id @default(cuid())
  createAt  DateTime @default(now())

  voteFor     Int
  voteAgainst Int
}